(jbuild_version 1)

(executables
 ((names        (simple Rosenbrock error unconstrained))
  (libraries    (lbfgs lacaml))))

(alias
 ((name runtest)
  (deps (simple.exe))
  (action (run ${<}))))

(alias
 ((name runtest)
  (deps (Rosenbrock.exe))
  (action (run ${<}))))

(alias
 ((name runtest)
  (deps (error.exe))
  (action (run ${<}))))

(alias
 ((name runtest)
  (deps (unconstrained.exe))
  (action (run ${<}))))
